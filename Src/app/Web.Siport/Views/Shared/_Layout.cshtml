<!DOCTYPE html>
<html lang="es">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="user-scalable=no, width=device-width, initial-scale=1" />

    <title>@ViewBag.Title</title>

    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/normalize.css")" />
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/bootstrap/bootstrap.css")" />
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/bootstrap/bootstrap-theme.css")" />

    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/bootstrap.extension.css")" />
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/themes/base/jquery-ui.min.css")" />
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/themes/base/jquery-ui.structure.min.css")" />
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/themes/base/jquery-ui.theme.min.css")" />
    
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/jqgrid/ui.jqgrid.css")" />
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/jqgrid.extension.css")" />

     <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/website.css")" />

    <script src="@Url.Content("~/Scripts/modernizr-2.6.2.js")" type="text/javascript"></script>

    <script src="@Url.Content("~/Scripts/jquery-1.11.3.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/bootstrap.js")" type="text/javascript"></script>


    <script src="@Url.Content("~/Scripts/jquery-ui.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jqgrid/jquery.jqGrid.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jqgrid/i18n/grid.locale-es.js")" type="text/javascript"></script>

    @RenderSection("scripts", required: false)
    
</head>
<body>
    <header>
        <nav class="navbar navbar-inverse navbar-static-top" role="navigation">
            <div class="container">
                <div class="navbar-header">
                    <!--Aqui el menu-->
                </div>
            </div>
        </nav>
    </header>  
    <section class="jumbotron jumbotron-min">
        <div class="container">
            <div class="row">
                <div class="col-lg-11">
                    <h3>SIPORT - <span class="label">Sistema Inteligente ....</span></h3>
                </div>
            </div>
        </div>
    </section>
    <section class="main container">
        <div class="row">
            @RenderBody()
        </div>
    </section>

    <footer>
        <div class="container">
            <div class="row">
                <div class="col-xs-6">
                    <p>Gustavo Cuentas - Diseño de muestra</p>
                </div>
                <div class="col-xs-6">
                    <ul class="list-inline text-right">
                        <li><a href="#">Inicio</a></li>
                        <li><a href="#">Ingresar</a></li>
                        <li><a href="#">Contacto</a></li>
                    </ul>
                </div>
            </div>
        </div>
    </footer>

  

   
</body>
</html>
