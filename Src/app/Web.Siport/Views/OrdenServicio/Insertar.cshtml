@model  Web.Siport.Models.OrdenServicio.OrdenServicioCabeceraModel

@{
    ViewBag.Title = "Insertar Orden de Servicio";
   

}
@section scripts {
    <script src="@Url.Content("~/Scripts/jsview/ordenservicio.editar.js")" type="text/javascript"></script>
    <script type="text/javascript" src="http://maps.google.com/maps/api/js?sensor=true"></script>
    <script src="@Url.Content("~/Scripts/jsview/googlemap.js")" type="text/javascript"></script>
}



<div class="col-lg-12">
    <div class="well well-sm">
        @using (Html.BeginForm("Insertar", "OrdenServicio", FormMethod.Post, new { id = "formMain", @class = "form-horizontal", @role = "form" }))
        {
            @Html.HiddenFor(model => model.IdOrdenServicio)
            @Html.HiddenFor(model => model.IdGuid)
            <fieldset>
                <legend class="text-left header">Registrar Orden de Servicio</legend>
                @Html.Partial("_OrdenServicioDatos")
            </fieldset>

            <div class="container">
                <div class="pull-right">
                    <button id="btnguardarordserv" type="submit" class="btn btn-primary btn-sm" >
                        <span class="glyphicon glyphicon-ok" aria-hidden="true"></span> Guardar
                    </button>
                    <button id="btncancelarordserv" type="button" class="btn btn-default btn-sm">
                        <span class="glyphicon glyphicon-off" aria-hidden="true"></span> Cancelar
                    </button>
                </div>
            </div>
        }
    </div>
</div>

<!--Modal-->
<div class="modal fade" id="containermodal" tabindex="-1" role="dialog">
    <div class="modal-dialog" role="document">
        <div class="modal-content"></div>
    </div>
</div>



    




