@model Web.Siport.Models.HojaRuta.PlanificacionRutaCabModel
@{
    ViewBag.Title = "Planificar Hoja de Ruta";
}
@section scripts {
    <script src="@Url.Content("~/Scripts/jsview/hojaderuta.planificar.js")" type="text/javascript"></script>
    <script type="text/javascript" src="http://maps.google.com/maps/api/js?sensor=true"></script>
    <script src="@Url.Content("~/Scripts/jsview/googlemap.js")" type="text/javascript"></script>
}
<div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
    <div class="well well-sm">
        <fieldset>
            <legend class="text-left header">Registrar Planificación de Ruta</legend>
            
            @using (Html.BeginForm("IngresarPlanificacionRuta", "HojaRuta", FormMethod.Post, new { id = "formMain", @class = "form-horizontal" }))
            {
                @Html.Partial("_PlanificacionRutaBusqueda", Model)
            }
        </fieldset>
    </div>
</div>
<script language="javascript" type="text/javascript">
    $(document).ready(function () {
        initialize();
    });
</script>